{
  "name": "demo-react",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "quick": "concurrently \"npm run backend\" \"npm start\"",
    "cypress": "npx cypress open",
    "e2e": "concurrently \"npm run quick\" \"npm run cypress\"",
    "backend": "json-server --watch db.json --routes routes.json --port 8627 --middlewares ./node_modules/json-server-reset",
    "clean": "rimraf build",
    "start-webpack": "npm run clean && webpack-dev-server --config=webpack.dev.js",
    "build-webpack": "npm run clean && webpack --config=webpack.prod.js",
    "quick-webpack": "concurrently \"npm run backend\" \"npm start-webpack\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@reduxjs/toolkit": "^1.4.0",
    "axios": "^0.19.0",
    "bulma": "^0.8.0",
    "http-proxy-middleware": "^1.0.3",
    "node-sass": "^4.13.1",
    "react": "^16.8.6",
    "bootstrap": "^4.5.2",
    "react-bootstrap": "^1.3.0",
    "react-datepicker": "^3.1.3",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^3.3.0",
    "react-select": "^3.1.0",
    "redux": "^4.0.4",
    "redux-saga": "^1.0.5",
    "redux-thunk": "^2.3.0",
    "react-toastify": "^6.0.8",
    "moment": "^2.28.0",
    "moment-timezone": "^0.5.31",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@types/express": "^4.17.8",
    "@types/lodash": "^4.14.161",
    "@types/node": "^14.11.2",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-select": "^3.0.19",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-react-app": "^9.1.2",
    "concurrently": "^5.1.0",
    "css-loader": "^3.4.2",
    "cypress": "^4.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.10.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
